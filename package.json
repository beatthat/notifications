{
  "name": "notifications",
  "scope": "ape",
  "version": "0.0.10",
  "description": "Global system for pub/sub notification events by string 'type'.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beatthat/unity-notifications.git"
  },
  "files": [
    "index.js",
    "scripts",
    "src"
  ],
  "author": "larrykirschner@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beatthat/unity-notifications/issues"
  },
  "homepage": "https://github.com/beatthat/unity-notifications",
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "install:test": "rm -f *.tgz && npm pack && cd test && npm run clean && npm install",
    "clean": "rm -f *.tgz && cd test && npm run clean",
    "sync:test2src": "rsync -rv --include '*/' --include '*.cs' --exclude '*' --prune-empty-dirs test/Assets/Plugins/${npm_package_scope}/${npm_package_name}/ ./src/${npm_package_name}/"
  },
  "dependencies": {
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "bindings": "beatthat/bindings#semver:^0.0.4",
    "pools": "beatthat/pools#semver:^0.0.7",
    "type-utils": "beatthat/type-utils#semver:^0.0.5"
  }
}
